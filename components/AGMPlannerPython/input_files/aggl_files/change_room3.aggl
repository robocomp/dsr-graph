askForIndividualPermission : active(1)
{
	{
		p:person(-40,-50)
		robot:robot(-195,55)
		p->p(is_blocking)
		p->p(is_near)
		p->robot(front)
		robot->robot(blocked)
	}
	=>
	{
		p:person(10,-115)
		robot:robot(-140,10)
		p->p(is_near)
		p->robot(front)
	}
	precondition
	{
		(forall 
			otherp:person
			(when   
				(not (= p otherp))
				(not (is_blocking otherp otherp))
			)       
		)
	}
}

